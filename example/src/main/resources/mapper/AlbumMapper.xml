<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.xiezc.example.web.mapper.AlbumMapper">

    <!--查询单个-->
    <select id="queryById" resultMap="XZCBaseResultMap">
        select id,
               title,
               create_time
        from t_album
        where id = #{id}
    </select>


</mapper>